{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "rewrites": [
    { 
      "source": "/camp", 
      "destination": "https://gbfcamp.paultharun.com" 
    },
    { 
      "source": "/camp/:match*", 
      "destination": "https://gbfcamp.paultharun.com/:match*" 
    }
  ],
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
